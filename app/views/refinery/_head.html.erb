<head>
  <meta charset='<%= Rails.application.config.encoding %>' />
  <!--[if IE]><meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" /><![endif]-->
  <title><%= browser_title(yield(:title)) %></title>
  <meta name='Description' content='Strona internetowa Miejskiego Domu Kultury w Radomsku.' />
  <meta name='Keywords' content='teatr, kino, wystawy, Miejski Dom Kultury, MDK, Radomsko, Brzeźnicka, kultura, Teatr im. Jaracza w Łodzi' />
  <meta name='email' content='biuro@mdk.radomsko.pl' />
  <meta http-equiv='Revisit-After' content='1 days' />
  <meta name='Robots' content='index, follow' />
  <meta name="google-site-verification" content="" />
  <%= raw %(<link rel="canonical" content="#{@canonical}" />) if @canonical.present? -%>
  <%= csrf_meta_tags if Refinery::Core.authenticity_token_on_frontend -%>
  <%= yield :meta %>

  <%= stylesheet_link_tag "application", "formatting", "theme" %>
  <%= stylesheet_link_tag "home" if home_page? %>
  <%= yield :stylesheets %>

  <%= render '/refinery/google_analytics' %>

  <%= javascript_include_tag 'modernizr-min' %>
</head>
